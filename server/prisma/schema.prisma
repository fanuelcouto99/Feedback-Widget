generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

// Criando a tabela para o banco de dados
model Feedback {

  // Definindo id como primary key e usando id como uuid
  id String @id @default(uuid())

  type       String
  comment    String
  screenshot String?

  // Dando nome para a tabela
  @@map("feedbacks")
}
